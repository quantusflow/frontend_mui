{
  "presets": ["react", "es2015", "stage-0"],
  "sourceMaps": true,
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name",
    "syntax-async-functions",
    "transform-regenerator",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}